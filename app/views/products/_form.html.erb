<br/><br/>
<div>
  <div class="large-7 columns">
    <%= simple_form_for @product do |f| %>
      <%= f.input :name %>
      <%= f.label :food_photo, "Food Item Photo" %>
      <%= f.file_field :image %>
      <%= f.input :description %>
      <%= f.input :price %>
      <%= f.button :submit, 'Add food item' %>
    <% end %>
  </div>
</div>
